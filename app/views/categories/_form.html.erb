<%= form_with(model: @category, data: { turbo: "false" }) do |f| %>
    <div class="mb-3 ">
        <%= f.label :name,"Category Name" ,class:"form-label"%><br />
            <%= f.text_field :name ,class:"form-control",placeholder:"enter name....." %>
    </div>


    <div class="mb-3">
        <%= f.submit class:"btn btn-outline-success"%>
    </div>
    <%end%>